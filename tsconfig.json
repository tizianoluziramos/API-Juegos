{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "pretty": false,
    "incremental": true,
    "allowJs": false,
    "types": ["node"],
    "sourceMap": false,
    "removeComments": true
  }
}
